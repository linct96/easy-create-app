{
  "name": "<%= name %>",
  "description": "<%= description %>",
  "version": "<%= version %>",
  "main": "<%= main %>",
  "repository": "<%= repository %>",
  "author": "<%= author %>",
  "license": "<%= license %>",
  "dependencies": {<% dependencies.forEach((element,index) => { %>
    "<%= element.name %>":"<%= element.version %>" <% if (dependencies.length!==index+1) { %>,<% } %> <% }) %>
  },
  "devDependencies": {<% devDependencies.forEach((element,index) => { %>
    "<%= element.name %>":"<%= element.version %>" <% if (devDependencies.length!==index+1) { %>,<% } %> <% }) %>
  }
}